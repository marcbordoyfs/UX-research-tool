<Window x:Class="UXTool_V4.MessageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UXTool_V4"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MessageWindow" Height="100" Width="300">
    <!--Loaded="Window_Loaded"-->
    
    <Window.Content>
        <TextBlock x:Name="Message" HorizontalAlignment="Center" TextWrapping="Wrap" Text="TextBlock" FontSize="16" VerticalAlignment="Center"/>
    </Window.Content>
    <!--<Grid Margin="40,40,40,40">
       

    </Grid>-->
</Window>
